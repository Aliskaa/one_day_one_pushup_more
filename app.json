{
  "expo": {
    "name": "1 Day 1 Pushup+",
    "slug": "one-day-one-pushup-more",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon-foreground.png",
    "scheme": "onedayonepushupmore",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/adaptive-icon-foreground.png",
      "resizeMode": "contain",
      "backgroundImage": "./assets/images/adaptive-icon-background.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-foreground.png",
        "backgroundImage": "./assets/images/adaptive-icon-background.png"
      },
      "package": "com.aliskaa.onedayonepushupmore",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "NOTIFICATIONS",
        "SCHEDULE_EXACT_ALARM",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_one_day_one_pushup_more.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo_one_day_one_pushup_more.png",
          "color": "#ffffff",
          "sounds": []
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3ecf13a9-fabf-42eb-a401-23aad62346e4"
      }
    }
  }
}
